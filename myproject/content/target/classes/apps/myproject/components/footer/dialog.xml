<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Footer description Component"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Edit footer">
                <items jcr:primaryType="cq:WidgetCollection">
                    <footer
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Enter the developer name"
                        fieldLabel="Developed by"
                        name="./custom-title"
                        xtype="textfield"/>
                    <company
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Enter the name of the company"
                        fieldLabel="Company"
                        name="./jcr:description"
                        xtype="textfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>

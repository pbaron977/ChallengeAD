<html>
    <head>
    	<script>
    		var actual=0;
    		$(window).ready(function() {
    			loadPage();
    		});
    	</script>
    	<script>
    		function loadPage(){
    			var first = actual;
    			var lengthVar = 5;
    			$.post('/bin/list', {
    				firstIndex : first,
    				length: lengthVar,
    			}, function(responseText) {
    				$('#data').html(responseText);
    			});
    		}
    		function avPag(){
    			actual++;
    			loadPage();
    		}
    		function rePag(){
    			actual--;
    			loadPage();
    		}
    	</script>
    </head>
    <body>
       	<cq:include script="head.jsp"/>
 		<cq:include path="parsys" resourceType="foundation/components/parsys"/>
        <div align="north">
            <cq:include path="head"  resourceType="adchall/components/headerComp" />
        </div>
        <div align="center">
            <cq:include path="content" resourceType="adchall/components/body" />
        </div>
        <div align="south">
            <cq:include path="footer" resourceType="adchall/components/footerComp" />
        </div>
    </body>
